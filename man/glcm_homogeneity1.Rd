function (glcm) 
{
    sum <- 0
    for (i in 1:nrow(glcm)) {
        for (j in 1:ncol(glcm)) {
            sum <- sum + glcm[i, j]/(1 + (abs(i - j)))
        }
    }
    return(sum)
  }
}
